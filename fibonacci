package lab.pkg3;
import java.util.Scanner;
public class Lab3 {
public static void main(String[] args) {
Scanner in= new Scanner(System.in);
System.out.println("Enter the limit");
int limit=in.nextInt(); // limit stored to the variable "limit"
int t1 = 0, t2 = 1; //Initializing the variables t1,t2
System.out.print("First " + limit + " terms: ");
for (int i = 1; i <= limit; ++i)
{
System.out.print(t1 + " , "); //output values separated by Comma
int sum = t1 + t2;
t1 = t2;
t2 = sum;
}
}
}